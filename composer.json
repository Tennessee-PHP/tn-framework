{
    "name": "tennessee-php/tn-framework",
    "description": "A PHP Framework from the volunteer state",
    "type": "library",
    "config": {
        "platform": {
            "php": "8.3"
        }
    },
    "require": {
        "league/climate": "^3.10",
        "nikic/php-parser": "^5.3.1",
        "predis/predis": "^2.2.2",
        "smarty/smarty": "^v5.4.1",
        "vlucas/phpdotenv": "^5.3",
        "ramsey/uuid": "^4.2",
        "ext-curl": "*",
        "ext-pdo": "*",
        "guzzlehttp/guzzle": "^7.9.2",
        "brick/math": "0.12.1"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.39"
    },
    "autoload": {
        "psr-4": {
            "TN\\": "src/TN/"
        }
    }
}
